{
    "version": 2,
    "builds": [
      {
        "src": "myresume/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb",
          "runtime": "python3.10.7" 
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "myresume/wsgi.py"
      }
    ]
    
  }
  